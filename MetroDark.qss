 */
* {
  font-family: "Segoe UI", Frutiger, "Frutiger Linotype", "Dejavu Sans", "Helvetica Neue", Arial, sans-serif;
  font-size: 14px;
  margin: 0;
  padding: 0;
  outline: 0;
  border: 0;
  selection-background-color: #298ce1;
  selection-color: white;
}

DockTitleBar {
  font-size: 11px;
}

QMainWindow,
QDockWidget {
  background-color: #2c2c2c;
}

QToolTip,
QWhatsThis {
  border: 1px solid #1c1c1c;
  background-color: #2c2c2c;
  color: #cccccc;
}

QTreeView.UserView,
QTextBrowser.LogTextBrowser {
  min-height: 120px;
  min-width: 250px;
  padding: 1px;
  margin: 0px;
}

QTreeView.UserView {
  min-width: 50px;
}

QMainWindow::separator {
  background-color: #2c2c2c;
  height: 4px;
  width: 4px;
}

QMainWindow::separator:hover {
  background: #383838;
}

QDialog,
QWizard *,
QCalendarWidget *,
#qswPages > QObject {
  background-color: #222222;
  color: #cccccc;
}

QTextEdit {
  background-color: #2c2c2c;
  color: #cccccc;
}

QTextEdit:disabled {
  color: #444444;
}

#qdwChat QTextEdit {
  font-size: 12px;
}

a {
  color: #39a5dd;
  text-decoration: none;
}

QToolBar {
  background-color: #2c2c2c;
  spacing: 0;
}

QToolBar::separator {
  background: #555555;
  height: 1px;
  margin: 4px;
  width: 1px;
}

QToolBar::handle {
  padding: 4px;
}

QToolBar::handle:horizontal {
  image: url(skin:controls/handle_horizontal.svg);
  width: 8px;
}

QToolBar::handle:vertical {
  height: 8px;
  image: url(skin:controls/handle_vertical.svg);
}

QToolButton {
  padding: 4px;
}

QToolButton:hover {
  background-color: #383838;
}

QToolButton:pressed {
  background-color: #484848;
}

QToolButton:on {
  background-color: #383838;
}

QToolButton:on:pressed {
  background-color: #484848;
}

QToolButton#qt_toolbar_ext_button {
  width: 10px;
  padding: 1px;
  border: 0;
  qproperty-icon: url(skin:controls/toolbar_ext.svg);
}

QHeaderView {
  border: 0;
  border-bottom: 1px solid #1c1c1c;
}

QHeaderView::section {
  background-color: #2c2c2c;
  border: 0;
  color: #dddddd;
  padding: 4px;
  padding-left: 8px;
}

QHeaderView::down-arrow {
  margin: 1px;
  top: 1px;
  right: 10px;
  width: 14px;
  image: url(skin:controls/arrow_down.svg);
}

QHeaderView::up-arrow {
  margin: 1px;
  top: 1px;
  right: 10px;
  width: 14px;
  image: url(skin:controls/arrow_up.svg);
}

QTreeView {
  background-color: #1c1c1c;
  alternate-background-color: transparent;
  border: 1px solid #1c1c1c;
  color: #dddddd;
  outline: 0;
  selection-background-color: #1c1c1c;
  selection-color: #dddddd;
  margin: 4px;
}

QTreeView::item {
  border-left: 1px solid transparent;
  padding: 4px;
}

QTreeView.UserView::item {
  padding: 0;
}

QTreeView::item:hover {
  background-color: #2c2c2c;
  border-top: 1px transparent;
}

QTreeView::item:selected {
  background-color: #2c2c2c;
  border: 1px solid #1c1c1c;
  border-right: 1px solid transparent;
  border-left: 1px solid transparent;
}

QTreeView#qtwServers::item:selected {
  border-left: 1px solid #1c1c1c;
  border-right: 1px solid #1c1c1c;
}

QTreeView::item:selected.UserView {
  border: 1px solid #1c1c1c;
}

QTreeView::branch {
  border-image: none;
  image: none;
  margin-left: 3px;
  margin-top: 1px;
  padding-left: 3px;
}

QTreeView::branch:has-children:closed {
  image: url(skin:controls/branch_closed.svg);
}

QTreeView::branch:has-children:open {
  image: url(skin:controls/branch_open.svg);
}

QTextBrowser {
  background-color: #1c1c1c;
  border: 1px solid #1c1c1c;
  color: #dddddd;
  padding: 1px;
}

.log-time {
  background-color: transparent;
  color: #95a5a6;
  font-size: 14px;
}

.log-server {
  background-color: transparent;
  color: #F9655D;
  font-weight: bold;
}

.log-channel {
  background-color: transparent;
  color: #e67e22;
  font-weight: bold;
}

.log-privilege {
  background-color: transparent;
  color: #c0392b;
  font-weight: bold;
}

.log-target {
  background-color: transparent;
  color: #27ae60;
  font-weight: bold;
}

.log-source {
  background-color: transparent;
  color: #27ae60;
  font-weight: bold;
}

QScrollBar:horizontal {
  background: transparent;
  margin-top: 1px;
  height: 15px;
}

QScrollBar:vertical {
  background: transparent;
  margin-left: 1px;
  width: 15px;
}

UserView QScrollBar:horizontal,
LogTextBrowser QScrollBar:horizontal {
  margin-top: 1px;
}

UserView QScrollBar:vertical,
LogTextBrowser QScrollBar:vertical {
  margin-left: 1px;
}

QScrollBar::handle {
  background: #555555;
  min-width: 10px;
  min-height: 10px;
}

QScrollBar::handle:hover {
  background: #888888;
}

QScrollBar::left-arrow,
QScrollBar::right-arrow,
QScrollBar::up-arrow,
QScrollBar::down-arrow,
QScrollBar::sub-line,
QScrollBar::add-line,
QScrollBar::add-page,
QScrollBar::sub-page {
  background: #2c2c2c;
  height: 0;
  width: 0;
}

QAbstractScrollArea::corner {
  background: transparent;
  height: 0;
  width: 0;
}

QMenuBar {
  background-color: #2c2c2c;
  border: 0;
}

QMenuBar::item {
  background-color: transparent;
  color: #cccccc;
  padding: 4px 12px;
  border: 0;
}

QMenuBar::item:selected {
  background: #298ce1;
  color: white;
}

QMenuBar::item:pressed {
  background: #1979ca;
  color: white;
}

QMenu {
  background: #2b2b2b;
  border: 1px solid #1c1c1c;
  color: #dddddd;
  padding: 0;
}

QMenu::item {
  background: transparent;
  border: 1px solid transparent;
  color: #dddddd;
  margin: 1px;
  padding: 4px 16px;
  padding-left: 25px;
}

QMenu::item:selected {
  background: #444444;
  border: 1px solid #444444;
}

QMenu::item:disabled {
  border: 1px solid transparent;
  background: transparent;
  color: #444444;
}

QMenu::separator {
  background: #555555;
  height: 1px;
}

QMenu::indicator {
  height: 20px;
  width: 25px;
}

QPushButton {
  background-color: #383838;
  border: 1px solid #272727;
  color: #dddddd;
  font-size: 16px;
  padding: 4px 20px;
  outline: 0;
}

QPushButton:hover {
  background-color: #484848;
}

QPushButton:pressed {
  background-color: #298ce1;
  color: white;
}

QGroupBox,
qwWarningContainer {
  background-color: #2c2c2c;
  border: 1px solid #1c1c1c;
  color: #cccccc;
  font-size: 18px;
  margin: 4px;
  padding: 4px;
  padding-top: 30px;
}

QGroupBox::title {
  background-color: transparent;
  margin: 4px;
  margin-left: 8px;
  margin-right: 8px;
}

QListView {
  background-color: #1c1c1c;
  alternate-background-color: #1c1c1c;
  border: 1px solid #1c1c1c;
  outline: 0;
  padding-top: -1px;
}

QListView#qlwIcons {
  padding: 0;
  background-color: transparent;
  border: 0;
  min-width: 160px;
  font-size: 16px;
  margin-left: 4px;
  margin-top: 15px;
}

QListView::item {
  border-left: 8px solid transparent;
  color: #cccccc;
  padding: 4px 0;
  border-top: 1px solid transparent;
}

QListView#qlwIcons::item {
  padding: 5px 0;
}

QListView::item:hover {
  border-top: 0;
  margin-top: 1px;
  background-color: #2c2c2c;
}

QListView#qlwIcons::item:hover {
  background-color: #383838;
}

QListView::item:selected {
  margin-top: 0;
  background-color: #2c2c2c;
  border: 1px solid #1c1c1c;
  border-left: 1px solid transparent;
  border-right: 1px solid transparent;
  padding-left: 7px;
}

QListView#qlwIcons::item:selected {
  margin-top: 1px;
  background-color: #2c2c2c;
  border: 1px solid #1c1c1c;
  padding-left: 7px;
}

QTabWidget::pane {
  background-color: #2c2c2c;
  border: 1px solid #1c1c1c;
  margin-left: 4px;
  margin-top: -1px;
}

QTabWidget::tab-bar {
  background-color: #222222;
  left: 4px;
}

QTabBar::tab {
  background-color: transparent;
  color: #dddddd;
  padding: 4px 20px;
  border-bottom: 1px solid #1c1c1c;
  border-right: 1px solid #1c1c1c;
  margin-left: -1px;
  height: 20px;
  subcontrol-position: center bottom;
}

QTabBar::tab:first {
  border-left: 1px solid transparent;
  margin: 0;
}

QTabBar::tab:last {
  border-right: 1px solid transparent;
}

QTabBar::tab:hover {
  background-color: #383838;
}

QTabBar::tab:disabled {
  color: #444444;
}

QTabBar::tab:selected {
  height: 20px;
  background-color: #2c2c2c;
  border: 1px solid #1c1c1c;
  border-bottom: 1px solid transparent;
  color: #dddddd;
}

RichTextEditor::pane {
  margin-top: 0;
  margin-bottom: -1px;
}

RichTextEditor QTabBar::tab {
  subcontrol-position: center top;
  border-top: 1px solid #1c1c1c;
  border-bottom: 1px solid transparent;
}

RichTextEditor QTabBar::tab:selected {
  border-top: 1px solid transparent;
  border-bottom: 1px solid #1c1c1c;
}

QComboBox,
QSpinBox,
QAbstractSpinBox {
  background-color: #383838;
}

QLineEdit {
  background-color: #383838;
}

QLineEdit,
QComboBox,
QSpinBox,
QAbstractSpinBox {
  border: 1px solid #272727;
  color: #dddddd;
  padding: 4px;
}

QSpinBox,
QAbstractSpinBox {
  min-width: 35px;
  padding-right: 15px;
}

QPushButton:disabled,
QLineEdit:disabled,
QComboBox:disabled,
QSpinBox:disabled,
QAbstractSpinBox:disabled {
  border: 0;
  margin: 1px;
  color: #444444;
  background-color: #282828;
}

QComboBox::drop-down,
QAbstractSpinBox::drop-down,
QSpinBox::drop-down,
QDateTimeEdit::drop-down {
  border: 0;
  margin-left: 4px;
  margin-right: 12px;
  margin-top: 5px;
}

QComboBox::down-arrow,
QDateTimeEdit::down-arrow {
  margin-top: -2px;
  image: url(skin:controls/arrow_down.svg);
  width: 14px;
}

QComboBox::down-arrow:disabled,
QDateTimeEdit::down-arrow:disabled {
  image: url(skin:controls/arrow_down_disabled.svg);
}

QSpinBox::down-button,
QAbstractSpinBox::down-button {
  padding-right: 4px;
  image: url(skin:controls/arrow_down.svg);
  width: 14px;
  padding-bottom: 1px;
}

QSpinBox::down-button:disabled,
QAbstractSpinBox::down-button:disabled {
  image: url(skin:controls/arrow_down_disabled.svg);
}

QSpinBox::up-button,
QAbstractSpinBox::up-button {
  padding-right: 4px;
  image: url(skin:controls/arrow_up.svg);
  width: 14px;
  padding-top: 1px;
}

QSpinBox::up-button:disabled,
QAbstractSpinBox::up-button:disabled {
  image: url(skin:controls/arrow_up_disabled.svg);
}

QComboBox QAbstractItemView {
  background-color: #1c1c1c;
  border: 1px solid #1c1c1c;
  color: #dddddd;
}

QLabel,
QCheckBox,
QAbstractCheckBox,
QTreeView::indicator,
QRadioButton {
  color: #dddddd;
  background: transparent;
}

QLabel:disabled {
  color: #444444;
}

QCheckBox::indicator,
QTreeView::indicator {
  background-color: #383838;
  border: 1px solid #272727;
  height: 13px;
  width: 13px;
  margin-top: 1px;
}

QMenu::indicator {
  width: 12px;
  left: 6px;
}

QCheckBox::indicator:checked,
QMenu::indicator:checked,
QTreeView::indicator:checked {
  image: url(skin:controls/checkbox_check_dark.svg);
}

QCheckBox::indicator:disabled,
QTreeView::indicator:disabled {
  border: 1px solid #2c2c2c;
  background-color: #282828;
}

QCheckBox::indicator:checked:disabled,
QTreeView::indicator:checked:disabled {
  border: 1px solid transparent;
  image: url(skin:controls/checkbox_check_disabled.svg);
}

QRadioButton:disabled,
QCheckBox:disabled {
  color: #444444;
}

QRadioButton::indicator {
  background: #383838;
  border: 1px solid #272727;
  border-radius: 7px;
  height: 12px;
  width: 12px;
}

QRadioButton::indicator:disabled {
  background-color: #282828;
  margin: 1px;
  border: 1px solid transparent;
}

QRadioButton::indicator:checked {
  image: url(skin:controls/radio_check_dark.svg);
}

QRadioButton::indicator:checked:disabled {
  image: url(skin:controls/radio_check_disabled.svg);
}

QSlider::groove {
  background: #333333;
  border: 1px solid #272727;
}

QSlider::groove:horizontal {
  height: 6px;
}

QSlider::groove:vertical {
  width: 6px;
}

QSlider::groove:disabled {
  background: #282828;
  border: 1px solid transparent;
}

QSlider::handle {
  background: #666666;
  border: 1px solid #272727;
  border-radius: 2px;
}

QSlider::handle:horizontal {
  margin: -4px -1px;
  width: 20px;
}

QSlider::handle:vertical {
  margin: -1px -5px;
  height: 20px;
}

QSlider::handle:hover,
QSlider::handle:pressed {
  background: #777777;
}

QSlider::handle:disabled {
  background-color: #282828;
  border: 1px solid #282828;
}

QComboBox::text {
  background-color: transparent;
}

QCheckBox::indicator:hover,
QTreeView::indicator:hover,
QRadioButton::indicator:hover,
QComboBox:hover {
  background-color: #484848;
}

QComboBox:editable:hover {
  background-color: #383838;
}

#qteRichText, #qptePlainText, #qwMacWarning {
  color: #dddddd;
  background-color: #1c1c1c;
  border: 1px solid #1c1c1c;
  padding: 4px;
}

#qtbToolBar {
  border: 1px solid transparent;
  background: transparent;
}

QFontDialog {
  min-width: 600px;
  min-height: 400px;
}

#BanEditor {
  min-width: 600px;
}

#GlobalShortcutTarget {
  min-height: 600px;
}

ViewCert {
  min-height: 600px;
}

QColorLuminancePicker {
  color: #dddddd;
  background: transparent;
}
